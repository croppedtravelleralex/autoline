{
    "envId": "your-env-id",
    "version": "2.0",
    "framework": {
        "name": "autoline",
        "plugins": {
            "client": {
                "use": "@cloudbase/framework-plugin-website",
                "inputs": {
                    "outputPath": "frontend/dist"
                }
            },
            "server": {
                "use": "@cloudbase/framework-plugin-container",
                "inputs": {
                    "serviceName": "autoline-backend",
                    "servicePath": "/api",
                    "localPath": "./backend",
                    "containerPort": 8080,
                    "envVariables": {
                        "PORT": "8080"
                    },
                    "dockerfilePath": "Dockerfile",
                    "buildConfig": {
                        "image": "autoline-backend"
                    }
                }
            }
        }
    }
}