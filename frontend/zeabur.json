{
    "build": {
        "type": "static",
        "command": "npm install --force && npm run build",
        "output": "dist"
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "http://backend:8080/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}